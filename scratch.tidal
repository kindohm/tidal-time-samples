

d1 $ slowspread (slow) [16,8,16,4] $ (striate' 256 (1/50) $ sound "break")
|=| speed (every 2 rev $ "[2 1 -1 0.5, 3 5 4]")
|=| gain "1.2"

d1 $ every 3 (|*| speed "-1") $ slowspread (slow) [8,16,4,8,32] $
(spread' (striate' 256) (scale 0.01 0.05 $ slow 1.25 sine1) $ sound "break")
|=| speed "[1 0.5, {2 3 4}%2]"
|=| gain "1.2"
